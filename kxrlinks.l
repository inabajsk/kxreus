    (cond
      (double (setq blist (append (send b0 :bodies) (send b1 :bodies))))
      (t (send b1 :translate #f(0 0 6) :world)
	 (setq blist (append (cdr (send b0 :bodies)) (send b1 :bodies)))))
    (kxr-link-create blist :joint-coord c)))

