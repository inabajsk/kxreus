(defun kxr-create-screw (m l &optional (h 1.0) (m2 (+ m 1.5)))
  (let ((s (make-cylinder (/ m 2.0) (+ l 0.5)))
	(h (make-cylinder (/ m2 2.0) h)))
    (send s :locate #f(0 0 0.5))
    (body+ h s)))
